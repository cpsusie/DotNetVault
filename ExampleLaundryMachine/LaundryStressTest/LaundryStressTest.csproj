  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\LaundryMachineUi\LaundryMachineUi.csproj" />
    <ProjectReference Include="..\LaundryMachine\LaundryMachine.csproj">
      <IncludeAssets>analyzers</IncludeAssets>
    </ProjectReference>
  </ItemGroup>

